<!DOCTYPE html>
<html>
  <head>
    <title>Events</title>
    <%- include('./partials/links.ejs'); %>
    <link rel="stylesheet" href="/css/song.css" />
  </head>
  <body>
    <% let default_song = {soid: "DEFAULT_SOID", name: "DEFAULT_NAME", artist: "DEFAULT_ARTIST", friendly_name: "DEFAULT_FRIENDLYNAME", modification: "DEFAULT_MODIFICATION", updated: "DEFAULT_UPDATED", duration: "DEFAULT_DURATION", source: "DEFAULT_SOURCE" } %>
    <script>
        let song_template = `<%- include('./partials/config/song_item.ejs', {song: default_song}) %>`;
        
    </script>
    <%- include('./partials/header.ejs'); %>
    <script src="/js/song.js"></script>
    <div class="mainbodyoutside flex center">
        <div class="mainbody">
            <div class="song-vertical-flex">
                <div class="song-header">
                    <div class="flex center">
                        <span class="large spacing">Manage Song List</span>
                    </div>
                    
    
                    <span class="small spacing">Manage all of the available songs that can be selected for events. </span>
                    <div class="message message-warning">
                        <span class="small message-content">Removing songs can cause problems for both events and event templates.</span>
                    </div>
                    
                    
                    
                    
                </div>
                <div id="song-list" class="song-list">
                    <%- include('./partials/config/all_songs.ejs', {songs: songs}); %>
                </div>
            </div>
            
        </div>
        
    </div>
    <%- include('./partials/footer.ejs'); %>
  </body>
</html>